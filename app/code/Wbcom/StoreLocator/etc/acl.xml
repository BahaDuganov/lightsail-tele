<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Wbcom_Core::Wbcom" title="Wbcom" sortOrder="45">
                    <resource id="Wbcom_StoreLocator::store" title="Store Locator" sortOrder="20">
                        <resource id="Wbcom_StoreLocator::manage_store" title="Manage Store" sortOrder="40" />
