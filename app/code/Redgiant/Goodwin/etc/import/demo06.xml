        <custom_settings>
            <custom_style><![CDATA[.page-footer {
    background-color: #1d1d1d;
}
body:not(.cms-index-index) .page-footer {
    padding-top: 65px;
    margin-top: 0;
}
.page-header.type6 .top-bar {
    background-color: #e53d60;
}
.page-footer .switcher .options .dropdown {
    background: #1e1e1e;
    border-color: #2d2d2d;
}
.page-footer .footer-bottom,.page-footer .title h3, .page-footer .title .h3-style, .page-footer .title h4, .page-footer .title .h4-style, .page-footer .title h5, .page-footer .title .h5-style, .page-footer .title h6, .page-footer .title .h6-style, .page-footer .footer-top {
    color: #fff;
}
.page-footer a, .page-footer .footer-block a, .page-footer .footer-block ul li a, .page-footer .footer-block ul li a[href^="mailto:"] {
   color: #b3b3b3;
}
.page-footer .row.lined:before {
   background-color: #2d2d2d;
}
.page-footer .block.newsletter .form.subscribe {
   max-width: 360px;
   position: relative;
}
.page-footer .block.newsletter .field, .block.newsletter .field .control {
   display: block;
}
.page-footer .block.newsletter .field .control input {
    box-shadow: none;
    border-color: #464646;
    border-style: solid;
    border-width: 1px;
    outline: 0 none;
    background-color: #1d1d1d;
    border-radius: 0;
    color: #b3b3b3;
    font-size: 13px;
    height: 40px;
    line-height: 16px;
    padding: 12px 15px 12px;
    width: 100%;
}
.page-footer .block.newsletter .label {
   display: none;
}
.page-footer .block.newsletter .actions {
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
}
.page-footer .block.newsletter .action.subscribe.primary {
    font-size: 0;
    height: 40px;
    line-height: 40px;
    padding: 0 15px;
    background: transparent;
    transition: all 0.2s ease;
}
.page-footer .block.newsletter .action.subscribe.primary span:before {
    content: "\e929";
    font-family: icomoon;
    font-size: 20px;
}
.page-footer .block.newsletter .action.subscribe.primary:hover {
    background-color: #000;
    outline: 0;
}]]></custom_style>
